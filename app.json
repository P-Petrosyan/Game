{
  "expo": {
    "name": "Path Blocker Online",
    "slug": "path-blocker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon1.png",
    "scheme": "pathblocker",
    "extra": {
      "eas": {
        "projectId": "project-344529006287"
      }
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pathblocker",
      "deploymentTarget": "18.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon1.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.pathblocker",
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gameReady.png",
          "imageWidth": "100%",
          "imageHeight": "100%",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"

        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-4468002211413891~1278388928",
          "ios_app_id": "ca-app-pub-4468002211413891~5060585795"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
