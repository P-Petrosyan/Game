{
  "expo": {
    "name": "Quoridor Online",
    "slug": "quoridor-online",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon4.png",
    "scheme": "quoridoronline",
    "extra": {
      "eas": {
        "projectId": "project-344529006287"
      }
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pathblocker",
      "deploymentTarget": "18.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "GADApplicationIdentifier": "ca-app-pub-4468002211413891~5060585795",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon4.png",
        "backgroundImage": "./assets/images/icon4.png",
        "monochromeImage": "./assets/images/icon4.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.pathblocker"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon4.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gameReady.png",
          "imageWidth": "100%",
          "imageHeight": "100%",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"

        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
